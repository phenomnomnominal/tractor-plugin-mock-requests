{
  "name": "tractor-plugin-mock-requests",
  "version": "0.2.0-beta.1",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-mock-requests.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-mock-requests/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "npm run bundle:add-mocking && npm run bundle:fetch && npm run bundle:xhr",
    "bundle:add-mocking": "webpack src/server/script-entries/add-mocking.js dist/server/scripts/add-mocking.js",
    "bundle:fetch": "webpack src/server/script-entries/shim-fetch.js dist/server/scripts/shim-fetch.js",
    "bundle:xhr": "webpack src/server/script-entries/shim-xhr.js dist/server/scripts/shim-xhr.js",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js,src/server/script-entries",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/server/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "coveralls": "2.11.16",
    "dirty-chai": "1.2.2",
    "eslint": "3.15.0",
    "istanbul": "1.0.0-alpha.2",
    "mkdirp": "^0.5.1",
    "mocha": "3.2.0",
    "npm-watch": "0.1.8",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "webpack": "2.3.2"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cheerio": "^0.22.0",
    "express": "^4.15.2",
    "express-http-proxy": "^0.11.0",
    "fake-xml-http-request": "^1.4.0",
    "fetch-mock": "^5.9.4",
    "pem": "^1.9.4",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "tractor-error-handler": "^0.2.0",
    "tractor-logger": "^0.1.0"
  }
}
