{
  "name": "tractor-plugin-mock-requests",
  "version": "0.2.0-beta.6",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-mock-requests.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-mock-requests/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "webpack -p",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js,src/server/scripts",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/server/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.2.0",
    "istanbul": "1.0.0-alpha.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-watch": "^0.2.0",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cheerio": "^0.22.0",
    "dedent": "^0.7.0",
    "express": "^4.15.3",
    "express-http-proxy": "^1.0.5",
    "fake-xml-http-request": "^1.6.0",
    "fetch-mock": "^5.12.1",
    "pem": "^1.9.7",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "tractor-error-handler": "^0.2.0",
    "tractor-logger": "^0.1.0"
  }
}
